{
  "include": ["./src/**/*"],
  "outDir": "./release",
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "commonjs",
    "target": "es6",
    "allowJs": true,
    "baseUrl": "src",
    "skipLibCheck": true,
    "lib": ["dom"],
    "jsx": "react",
    "jsxFactory": "pluginscript",
    "moduleResolution": "node"
  }
}
